+_sms_aliases () {
+	local smsas
+	smsas=(${(f)"$(smsaddr -l)"})
+	smsas=(${smsas/	##/:})
+if [[ -n "$PREFIX$SUFFIX" ]]; then
+	smsas=($smsas ${smsas/(#s)(#b)(^($PREFIX)*):(*)(#e)/$match[3]:$match[1]})
+fi
+#	smsas=(${(M)smsas:#$PREFIX*})
+	_describe "SMS alias" smsas
+}
+
+_sms_alias () {
+	local smsas
+	smsas=(${(f)"$(smsaddr -l)"})
+	smsas=(${smsas/	##/:})
+	_describe "SMS alias" smsas
+}
